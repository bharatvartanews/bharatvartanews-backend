{
  "name": "bharat-varta-node-cms-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
  "dev": "ts-node-dev --respawn --transpile-only src/server.ts",

  "build": "node ./node_modules/prisma/build/index.js generate && node ./node_modules/typescript/bin/tsc"
,
  "start": "node dist/server.js",

  "prisma:generate": "node ./node_modules/prisma/build/index.js generate",
  "prisma:migrate:deploy": "node ./node_modules/prisma/build/index.js migrate deploy",
  "prisma:migrate:dev": "npx prisma migrate dev"
}

,
  "prisma": {
    "seed": "ts-node prisma/seed-all.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "prisma": "^5.22.0",
    "slugify": "^1.6.6",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"

  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
